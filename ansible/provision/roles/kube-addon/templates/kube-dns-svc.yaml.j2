apiVersion: v1
kind: Service
metadata:
  labels:
    component: kube-dns
    k8s-app: kube-dns
    kubernetes.io/cluster-service: "true"
    kubernetes.io/name: KubeDNS
    name: kube-dns
    tier: node
  name: kube-dns
  namespace: kube-system
spec:
  clusterIP: {{ kubernetes_dns_ip }}
  ports:
  - name: dns
    port: 53
    protocol: UDP
    targetPort: 53
  - name: dns-tcp
    port: 53
    protocol: TCP
    targetPort: 53
  selector:
    name: kube-dns
  sessionAffinity: None
  type: ClusterIP
