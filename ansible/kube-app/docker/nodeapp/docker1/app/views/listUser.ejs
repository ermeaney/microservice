<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">

    <header>
        <% include ../partials/header %>
    </header>

    <main>
        <div class="jumbotron">
            <h2>List of Users</h2>
            <table class="table table-bordered">
              <thead>
              <tr>
                <th>First Name</th>
                <th>Last Name</th>
              </tr>
              </thead>
              <tbody>
                <%  users.forEach(function(user){ %>
                 <tr>
                   <td><%= user.firstName %></td>
                   <td><%= user.lastName %></td>
                 </tr>
                <% }) %>
              </tbody>
            </table>
        </div>
    </main>

    <footer>
        <% include ../partials/footer %>
    </footer>
    
</body>
</html>
